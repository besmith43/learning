@inherits LayoutComponentBase
<MudThemeProvider/>
<MudDialogProvider/>
<MudSnackbarProvider/>

<div class="page">
    @* <div class="sidebar">
        <NavMenu />
    </div> *@

    <main>
        <div class="top-row px-4">
            <MudMenu Icon="fa-solid fa-compass" AnchorOrigin="Origin.BottomCenter" TransformOrigin="Origin.BottomCenter">
                <MudMenuItem Href="https://mudblazor.com/" Target="_blank">MudBlazor Docs</MudMenuItem>
                <MudMenuItem Href="https://mudblazor.com/getting-started/installation" Target="_blank">Install</MudMenuItem>
                <MudMenuItem Href="https://mudblazor.com/components" Target="_blank">Components</MudMenuItem>
            </MudMenu>
            <a href="https://learn.microsoft.com/aspnet/core/" target="_blank">About</a>
        </div>

        <article class="content px-4" style="background-color: #454d55;">
            @Body
        </article>
    </main>
</div>

<div id="blazor-error-ui">
    An unhandled error has occurred.
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>


