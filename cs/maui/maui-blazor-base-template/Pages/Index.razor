@page "/"

<h1>Hello, world!</h1>

Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?" />

@if (SwitchableLoad)
{
	<Switchable />
}


@code {

	public bool SwitchableLoad { get; set; } = false;

	protected override async Task OnInitializedAsync()
	{
		await Task.Delay(5000);
		SwitchableLoad = true;
		StateHasChanged();
	}
}